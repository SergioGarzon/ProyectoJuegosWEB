 <activities>
  <activity class="@puzzles.DoublePuzzle" name="Puzzle Conejito" code="">
   <description />
   <messages>
    <cell type="initial">
     <style borderStroke="0.7" markerStroke="5.0">
      <font family="Comic Sans MS" size="25" />
      <color background="0x999999" />
     </style>
     <p>Arma el rompecabezas y descubriras que sucede</p>
    </cell>
    <cell type="final">
     <style>
      <font family="Comic Sans MS" size="25" />
      <color background="0xFFFF00" />
     </style>
     <p>Â¡Felicitaciones! Esto es el planeta tierra contaminado</p>
    </cell>
    <cell type="finalError">
     <style>
      <font family="Comic Sans MS" size="25" />
      <color background="0xFF0000" />
     </style>
     <p>Perdiste :(</p>
    </cell>
   </messages>
   <settings margin="8" dragCells="true" report="true" reportActions="false">
    <container bgColor="0x0000FF">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <cells rows="3" cols="3" border="true" image="contaminacionAmbiental.png">
    <style />
    <shaper class="@Rectangular" cols="3" rows="3" />
   </cells>
   <scramble times="31" />
   <layout position="AB" />
  </activity>
